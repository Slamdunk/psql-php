{
    "name": "slam/psql-php",
    "description": "PHP version of psql cli that comes with PostgreSQL",
    "license": "MIT",
    "keywords": [
        "postgres",
        "psql"
    ],
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com"
        }
    ],
    "require": {
        "php": "~8.3.0 || ~8.4.0",
        "ext-pgsql": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75.0",
        "phpstan/phpstan": "^2",
        "phpstan/phpstan-phpunit": "^2",
        "phpstan/phpstan-strict-rules": "^2",
        "phpunit/phpunit": "^12.1.5"
    },
    "autoload": {
        "psr-4": {
            "SlamPsql\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamPsql\\Tests\\": "tests/"
        }
    },
    "bin": [
        "psql"
    ]
}
