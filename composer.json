{
    "name": "slam/psql-php",
    "description": "PHP version of psql cli that comes with PostgreSQL",
    "license": "MIT",
    "keywords": [
        "postgres",
        "psql"
    ],
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com"
        }
    ],
    "require": {
        "php": "~8.4.0 || ~8.5.0",
        "ext-pgsql": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.93.0",
        "phpstan/phpstan": "^2.1.37",
        "phpstan/phpstan-phpunit": "^2.0.12",
        "phpstan/phpstan-strict-rules": "^2.0.8",
        "phpunit/phpunit": "^12.5.8"
    },
    "autoload": {
        "psr-4": {
            "SlamPsql\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamPsql\\Tests\\": "tests/"
        }
    },
    "bin": [
        "psql"
    ]
}
