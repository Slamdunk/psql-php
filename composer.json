{
    "name": "slam/postgres-php",
    "description": "PHP version of postgres cli that comes with PostgreSQL",
    "license": "MIT",
    "keywords": [
        "postgres"
    ],
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-pgsql": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.25.1",
        "phpstan/phpstan": "^1.10.32",
        "phpstan/phpstan-phpunit": "^1.3.14",
        "phpstan/phpstan-strict-rules": "^1.5.1",
        "phpunit/phpunit": "^10.3.2"
    },
    "autoload": {
        "psr-4": {
            "SlamPgsql\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamPgsql\\Tests\\": "tests/"
        }
    },
    "bin": [
        "postgres"
    ]
}
